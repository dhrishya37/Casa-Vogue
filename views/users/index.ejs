<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" href="img/pages/icon.png" type="image/png">
    <title>Casa Vogue - Furniture</title>

    <!-- Vendor CSS -->
    <link rel="stylesheet" href="/vender/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="/vender/icofont/icofont.min.css">
    <link rel="stylesheet" href="/vender/aos/dist/aos.css">
    <link rel="stylesheet" href="/vender/remixicon/remixicon.css">

    <!-- Custom CSS -->
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/css/common.css">

</head>
<body class="bg-light">

    <!-- Page loading spinner -->
    <!-- <div class="page-loading active">
        <div class="page-loading-inner">
            <div class="page-spinner"></div>
            <span>Loading...</span>
        </div>
    </div> -->

    <div style="position: relative; background: url('/img/latest-blog/blog-4.jpg') center/cover no-repeat; max-height: 180vh; ">
        <div style=" z-index: 1;">

        <!-- Navbar -->
        <%- include('partial/navbar') %>
    
        <!-- Hero Section -->
        <div class="m-auto my-5">
            <div class="container py-2 my-7">
                <div class="row align-items-center g-4">
                    <div class="col-lg-6 col-12">
                        <h1 class="fw-bold text-white display-2" data-aos="fade-up" data-aos-duration="600">
                            Beautifully Crafted Modern Furniture
                        </h1>
                    </div>
                </div>
            </div>
        </div>
        </div>
    </div>
    <!-- Qualities Section -->
    <div class="pb-5">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="bg-white shadow p-5">
                        <div class="row g-4 g-md-5">
                            <div class="col-lg-4 col-12" data-aos="fade-up" data-aos-duration="500">
                                <div class="text-center">
                                    <i class="ri-thumb-up-line ri-3x text-secondary"></i>
                                    <div class="pt-3">
                                        <h5 class="fw-bold pb-2">High Quality</h5>
                                        <p class="text-muted mb-0">Top-notch craftsmanship and premium materials.</p>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-4 col-12" data-aos="fade-up" data-aos-duration="600">
                                <div class="text-center">
                                    <i class="ri-truck-line ri-3x text-secondary"></i>
                                    <div class="pt-3">
                                        <h5 class="fw-bold pb-2">Fast Delivery</h5>
                                        <p class="text-muted mb-0">Quick and safe delivery to your doorstep.</p>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-4 col-12" data-aos="fade-up" data-aos-duration="700">
                                <div class="text-center">
                                    <i class="ri-shield-check-line ri-3x text-secondary"></i>
                                    <div class="pt-3">
                                        <h5 class="fw-bold pb-2">Best Warranty</h5>
                                        <p class="text-muted mb-0">Guaranteed long-lasting durability.</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
    <!-- Footer -->
    <%- include('partial/footer') %>
