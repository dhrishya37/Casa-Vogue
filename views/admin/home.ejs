    <style>
        body {
            font-family: 'Inter', sans-serif;                                                                                                                                                                                                                                                                                                 
            margin: 0;
            padding: 0;
           
        }
        /* Navbar Styling */
        .navbar-vertical-nav {
            width: 260px;
            height: 100vh;
            background-color: hsl(0, 0%, 100%);
            border-right: 1px solid #cfcdcd;
            position: fixed;
            top: 0;
            left: 0;
            overflow-y: auto;
            box-shadow: 2px 0 10px rgba(0, 0, 0, 0.1);
            transition: all 0.3s ease;
        }
        
        .navbar-brand img {
            max-width: 160px;
            display: block;
            margin: 20px auto;
        }
        
        .navbar-nav {
            list-style: none;
            padding: 0;
            margin: 0;
        }
        
        .nav-item {
            display: block;
            border-bottom: 1px solid #f1f1f1;
        }
        
        .nav-link {
            display: flex;
            align-items: center;
            padding: 14px 24px;
            text-decoration: none;
            color: #333;
            font-size: 16px;
            font-weight: 500;
            transition: all 0.3s;
            border-radius: 6px;
        }
        
        .nav-link:hover, .nav-link:focus {
            background-color: #f8f9fa;
            color: #007bff;
        }
        
        .nav-link-icon {
            margin-right: 12px;
            font-size: 20px;
        }

        /* Main Content Styling */
        .main-content-wrapper {
            margin-left: 260px;
            padding: 20px;
            transition: margin-left 0.3s;
        }

        .container {
            max-width: 1200px;
            margin: auto;
        }

        .card {
            background-color: #ffffff;
            border-radius: 12px;
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.05);
            overflow: hidden;
        }

        .card-body {
            padding: 20px;
        }

        .btn {
            padding: 10px 16px;
            font-size: 14px;
            border-radius: 6px;
            transition: background 0.3s;
        }

        .btn:hover {
            opacity: 0.9;
        }

        /* Grid Layout */
        .grid {
            display: grid;
            gap: 20px;
        }

        .grid-cols-12 {
            grid-template-columns: repeat(12, 1fr);
        }

        .grid-cols-1 {
            grid-template-columns: repeat(1, 1fr);
        }

        .lg\:grid-cols-2 {
            grid-template-columns: repeat(2, 1fr);
        }

        .col-span-12 {
            grid-column: span 12;
        }

        .lg\:col-span-4 {
            grid-column: span 4;
        }

        .xl\:col-span-8 {
            grid-column: span 8;
        }

        /* Table Styling */
        table {
            width: 100%;
            border-collapse: collapse;
        }

        thead {
            background-color: #f4f5f7;
            color: #333;
        }

        td, th {
            padding: 12px;
            border-bottom: 1px solid #ddd;
        }

        .status-shipped {
            background-color: #d4edda;
            color: #155724;
            padding: 4px 8px;
            border-radius: 4px;
            font-size: 12px;
            font-weight: bold;
        }

        /* Responsive */
        @media (max-width: 1024px) {
            .navbar-vertical-nav {
                width: 100%;
                position: relative;
                box-shadow: none;
            }
            .main-content-wrapper {
                margin-left: 0;
            }
        }
    </style>
